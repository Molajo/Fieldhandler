{
    "name": "molajo/fieldhandler",
    "type": "molajo-package",
    "description": "Molajo Fieldhandler: Validates and Filters Input; Escapes and Formats Output",
    "keywords": [
        "Molajo",
        "Fieldhandler",
        "Filter",
        "Validate",
        "Escape",
        "Format"
    ],
    "homepage": "http://github.com/Molajo/Fieldhandler",
    "license": "MIT",
    "authors": [
        {
            "name": "Amy Stephen",
            "email": "AmyStephen@Molajo.org",
            "homepage": "http://AmyStephen.com"
        }
    ],
    "require": {
        "commonapi/exception": "dev-master"
    },
    "support": {
        "event": "hello@molajo.org",
